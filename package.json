{
  "name": "a7a-next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --check \"src/pages/**/*.tsx\"  \"src/theme/*.ts\" \"src/store/*.js\" \"styles/*.css\"",
    "format": "prettier --write \"src/pages/**/*.tsx\" \"src/theme/*.ts\" \"src/store/*.js\" \"src/styles/*.css\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/mdx": "^12.2.2",
    "cookies-js": "^1.2.3",
    "i18next": "^22.0.7",
    "markdown-to-txt": "^2.0.1",
    "next": "13.0.0",
    "next-i18next": "^13.2.2",
    "next-pwa": "^5.6.0",
    "next-redux-wrapper": "^7.0.5",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "react-logger": "^1.1.0",
    "react-persist": "^1.0.2",
    "react-redux": "^7.2.6",
    "react-responsive-carousel": "^3.2.23",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-cookie-storage": "^1.0.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.24",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "husky": "^8.0.0",
    "typescript": "4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": "prettier --write"
  }
}
